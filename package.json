{
  "name": "ytlh-oauth2-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development ts-node ./src/index.ts",
    "watch:page": "parcel watch -d public ./src/page/index.html",
    "build": "NODE_ENV=production parcel build ./src/index.ts --target node && parcel build ./src/page/index.html -d dist/public && rm ./dist/*.map",
    "start": "node ./dist/index.js",
    "format:fix": "prettier-eslint --write $PWD/'src/**/*.ts'"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "parcel-bundler": "^1.12.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "styled-jsx": "^4.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.11",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.9",
    "@types/styled-jsx": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.3.2",
    "prettier-eslint-cli": "^5.0.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
